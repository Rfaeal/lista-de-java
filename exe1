    public static void main(String[] args) {
        int[] numeros = new int[5];

        numeros[0] = 10;
        numeros[1] = 25;
        numeros[2] = 30;
        numeros[3] = 45;
        numeros[4] = 50;

        System.out.println("Os números no vetor são:");
        for (int i = 0; i < numeros.length; i++) {
            System.out.println("- " + numeros[i]);
        }
    }
}
