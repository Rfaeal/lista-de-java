public class SomaVetor {
    public static void main(String[] args) {
        int[] numeros = {5, 8, 12, 3, 7, 10};
        int soma = 0;

        for (int i = 0; i < numeros.length; i++) {
            soma += numeros[i];
        }

        System.out.println("A soma de todos os números é: " + soma);
    }
}
